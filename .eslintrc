{
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "airbnb-base"
    ],
    "plugins": ["prettier"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "array-callback-return": 0,
      "no-useless-escape": 0,
      "consistent-return": 0,
      "import/prefer-default-export": 0,
      "class-methods-use-this": 0,
      "no-param-reassign": 0
    }
  }
  
